{
  "compilerOptions": {
    "target": "ES2022",
    "module": "Node16",
    "moduleResolution": "node16",
    "declaration": false,
    "noEmit": true,
    "esModuleInterop": true,
    "strict": true,
    "lib": ["ESNext"],
    "types": ["vitest/globals"],
    "resolveJsonModule": true,
    "skipLibCheck": true
  },
  "exclude": ["dist"]
}
